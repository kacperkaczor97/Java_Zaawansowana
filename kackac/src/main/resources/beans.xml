<?xml version="1.0" encoding="utf-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="myFirstSimpleClass" class="pl.pjatk.kackac.MySimpleClass">
        <constructor-arg index="0" value="Hello from bean myFirstSimpleClass"/>
    </bean>

    <bean id="mySecondSimpleClass" class="pl.pjatk.kackac.MySimpleClass">
        <constructor-arg index="0" value="Hello from bean mySecondSimpleClass"/>
    </bean>
</beans>